<div class="row-fluid">
  <div class="container">
    <div id="content" class="span12">
      <div class="epiodes-heading">
        <h2>Videos</h2>
      </div>
      <div class="episodes">
        <% @video_links.each_slice(3) do |row| %>
          <div class="row-fluid">
          <% row.each do |video| %>
            <%= partial :'videos/video_link', :locals => {:video_link => video} %>
          <% end %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
